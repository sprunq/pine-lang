struct Test {
    step : i64,
    counter : i64
}

fun main() -> () {
    let iterator : Test = Test { step : 0, counter : 0 };

    loop {
        if (iterator.step == 2000000) {
            break;
        }

        if (iterator.step % 2 == 0) {
            iterator.counter = iterator.counter + 1;
        }
        iterator.step = iterator.step + 1;
    }
    print_int(iterator.counter);
}
